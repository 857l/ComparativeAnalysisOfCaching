<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ChatView">

        <attr name="auth" format="enum">
            <enum name="AUTH_WITH_FORM" value="0"/>
            <enum name="AUTH_WITHOUT_FORM" value="1"/>
        </attr>
        <attr name="timeDelayed" format="integer"/>
        <attr name="urlSocketNameSpace" format="string"/>
        <attr name="urlSocketHost" format="string"/>
        <attr name="urlUploadNameSpace" format="string"/>
        <attr name="urlUploadHost" format="string"/>

        <attr name="color_main" format="color"/>
        <attr name="color_bg_user_message" format="color"/>
        <attr name="color_bg_server_message" format="color"/>
        <attr name="color_bg_server_action" format="color"/>
        <attr name="color_borders_server_action" format="color"/>
        <attr name="color_text_user_message" format="color"/>
        <attr name="color_text_server_message" format="color"/>
        <attr name="color_text_server_action" format="color"/>
        <attr name="color_time_mark" format="color"/>
        <attr name="color_text_warning" format="color"/>
        <attr name="color_company" format="color"/>
        <attr name="color_text_date_grouping" format="color"/>

        <attr name="progressIndeterminateDrawable" format="reference"/>

        <attr name="size_user_message" format="dimension"/>
        <attr name="size_server_message" format="dimension"/>
        <attr name="size_server_action" format="dimension"/>
        <attr name="size_time_mark" format="dimension"/>
        <attr name="size_warning" format="dimension"/>
        <attr name="size_info" format="dimension"/>
        <attr name="size_text_date_grouping" format="dimension"/>

        <attr name="company_name" format="string"/>
        <attr name="show_company_name" format="boolean"/>
        <attr name="show_internet_connection_state" format="boolean"/>
        <attr name="show_upper_limiter" format="boolean"/>
        
    </declare-styleable>

</resources>